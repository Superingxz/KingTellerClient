<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/common_all_background"
    android:gravity="center"
    android:orientation="vertical" >
    
    <include layout="@layout/common_title_main_bar" />
    
	<RelativeLayout 
	    android:layout_width="match_parent"
	    android:layout_height="match_parent" >
	
	    <LinearLayout
	        android:id="@+id/linearLayout1"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:orientation="vertical" >
	
	        <LinearLayout
	            android:id="@+id/dj_service_information"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:background="@drawable/sdl_background"
	            android:orientation="vertical" >
	
	            <RelativeLayout
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_margin="@dimen/common_margin" >
	
	                <TextView
	                    android:id="@+id/dj_djdh"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentLeft="true"
	                    android:text="登记单号："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	
	                <TextView
	                    android:id="@+id/dj_djlczt"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentRight="true"
	                    android:text="流程状态："
	                    android:textColor="@color/orange"
	                    android:textSize="@dimen/common_text_small" />
	
	            </RelativeLayout>
	
	            <RelativeLayout
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_marginBottom="@dimen/common_margin"
	                android:layout_marginLeft="@dimen/common_margin"
	                android:layout_marginRight="@dimen/common_margin" >
	
	                <TextView
	                    android:id="@+id/dj_djcw"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentLeft="true"
	                    android:text="登记仓库："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	
	            </RelativeLayout>
	
	            <RelativeLayout
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_marginBottom="@dimen/common_margin"
	                android:layout_marginLeft="@dimen/common_margin"
	                android:layout_marginRight="@dimen/common_margin" >
	
	                <TextView
	                    android:id="@+id/dj_djckxz"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentLeft="true"
	                    android:text="出库性质："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	            </RelativeLayout>
	
	            <RelativeLayout
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_marginBottom="@dimen/common_margin"
	                android:layout_marginLeft="@dimen/common_margin"
	                android:layout_marginRight="@dimen/common_margin" >
	
	                <TextView
	                    android:id="@+id/dj_djrkxz"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentLeft="true"
	                    android:text="入库性质："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	            </RelativeLayout>
	
	            <LinearLayout
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_marginBottom="@dimen/common_margin"
	                android:layout_marginLeft="@dimen/common_margin"
	                android:layout_marginRight="@dimen/common_margin"
	                android:orientation="horizontal"
	                android:visibility="gone" >
	
	                <TextView
	                    android:id="@+id/dj_ydjckwuzs"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentLeft="true"
	                    android:layout_centerVertical="true"
	                    android:layout_weight="1"
	                    android:text="已登记出库物料总数："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	
	                <TextView
	                    android:id="@+id/dj_ydjckwltmzs"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentRight="true"
	                    android:layout_weight="1"
	                    android:text="物料条码总数："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	
	            </LinearLayout>
	
	            <LinearLayout
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_marginBottom="@dimen/common_margin"
	                android:layout_marginLeft="@dimen/common_margin"
	                android:layout_marginRight="@dimen/common_margin"
	                android:orientation="horizontal"
	                android:visibility="gone" >
	
	                <TextView
	                    android:id="@+id/dj_ydjrkwlzs"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentLeft="true"
	                    android:layout_centerVertical="true"
	                    android:layout_weight="1"
	                    android:text="已登记入库物料总数："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	
	                <TextView
	                    android:id="@+id/dj_ydjrkwltmzs"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentRight="true"
	                    android:layout_weight="1"
	                    android:text="物料条码总数："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	            </LinearLayout>
	
	            <RelativeLayout
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_marginBottom="@dimen/common_margin"
	                android:layout_marginLeft="@dimen/common_margin"
	                android:layout_marginRight="@dimen/common_margin" >
	
	                <TextView
	                    android:id="@+id/dj_djry"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentLeft="true"
	                    android:text="制单人员："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	
	                <TextView
	                    android:id="@+id/dj_djsj"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentRight="true"
	                    android:text="登记时间："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	            </RelativeLayout>
	        </LinearLayout>
	
	        <LinearLayout
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:background="@drawable/sdl_background" >
	
	            <Button
	                android:id="@+id/ewm_opencameck"
	                android:layout_width="wrap_content"
	                android:layout_height="@dimen/common_btn_height"
	                android:layout_alignParentLeft="true"
	                android:layout_alignParentRight="true"
	                android:layout_centerHorizontal="true"
	                android:layout_centerVertical="true"
	                android:layout_marginRight="@dimen/common_margin"
	                android:layout_weight="1"
	                android:background="@drawable/btn_red_bg"
	                android:minHeight="@null"
	                android:minWidth="@null"
	                android:text="物料出库扫描"
	                android:textColor="@color/white"
	                android:textSize="14sp" />
	
	            <Button
	                android:id="@+id/ewm_opencamerk"
	                android:layout_width="wrap_content"
	                android:layout_height="@dimen/common_btn_height"
	                android:layout_marginLeft="@dimen/common_margin"
	                android:layout_weight="1"
	                android:background="@drawable/btn_black_bg"
	                android:minHeight="@null"
	                android:minWidth="@null"
	                android:text="物料入库扫描"
	                android:textColor="@color/white"
	                android:textSize="14sp" />
	
	        </LinearLayout>
	    </LinearLayout>
	
	    <ExpandableListView
	        android:id="@+id/dj_expandableListView"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_above="@+id/ewm_btnlinearlayout"
	        android:layout_below="@+id/linearLayout1"
	        android:background="@drawable/sdl_background"
	        android:divider="@null"
	        android:groupIndicator="@null" >
	
	    </ExpandableListView>
	
	    <LinearLayout
	        android:id="@+id/ewm_btnlinearlayout"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_alignParentBottom="true"
	        android:padding="@dimen/common_padding5" >
	
	        <Button
	            android:id="@+id/ewm_cancel"
	            android:layout_width="wrap_content"
	            android:layout_height="@dimen/common_btn_height"
	            android:layout_marginRight="@dimen/common_margin"
	            android:layout_weight="1"
	            android:background="@drawable/btn_orange_bg"
	            android:minHeight="@null"
	            android:minWidth="@null"
	            android:text="关闭"
	            android:textColor="@color/white"
	            android:textSize="14sp" />
	
	        <Button
	            android:id="@+id/ewm_preservation"
	            android:layout_width="wrap_content"
	            android:layout_height="@dimen/common_btn_height"
	            android:layout_marginLeft="@dimen/common_margin"
	            android:layout_weight="1"
	            android:background="@drawable/btn_green_bg"
	            android:minHeight="@null"
	            android:minWidth="@null"
	            android:text="保存"
	            android:textColor="@color/white"
	            android:textSize="14sp" />

	    </LinearLayout>
	
	</RelativeLayout>
	
</LinearLayout>
