<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/common_all_background"
    android:gravity="center"
    android:orientation="vertical" >
    
    <include layout="@layout/common_title_main_bar" />
    
	<RelativeLayout 
	    android:layout_width="match_parent"
	    android:layout_height="match_parent">
	
	    <LinearLayout
	        android:id="@+id/linearLayout1"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:orientation="vertical" >
	
	        <LinearLayout
	            android:id="@+id/fh_invoice_information"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:background="@drawable/sdl_background"
	            android:orientation="vertical" >
	
	            <RelativeLayout
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_margin="@dimen/common_margin" >
	
	                <TextView
	                    android:id="@+id/fh_ckbh"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentLeft="true"
	                    android:text="出库编号："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	
	                <TextView
	                    android:id="@+id/fh_lczt"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentRight="true"
	                    android:text="流程状态："
	                    android:textColor="@color/orange"
	                    android:textSize="@dimen/common_text_small" />
	            </RelativeLayout>
	
	            <RelativeLayout
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_marginBottom="@dimen/common_margin"
	                android:layout_marginLeft="@dimen/common_margin"
	                android:layout_marginRight="@dimen/common_margin" >
	
	                <TextView
	                    android:id="@+id/fh_fhck"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentLeft="true"
	                    android:text="发货仓库："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	
	                <TextView
	                    android:id="@+id/fh_wlzs"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentRight="true"
	                    android:layout_alignParentTop="true"
	                    android:text="物料总数："
	                    android:textColor="#008B00"
	                    android:textSize="@dimen/common_text_small" />
	
	            </RelativeLayout>
	
	            <RelativeLayout
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_marginBottom="@dimen/common_margin"
	                android:layout_marginLeft="@dimen/common_margin"
	                android:layout_marginRight="@dimen/common_margin" >
	
	                <TextView
	                    android:id="@+id/fh_shck"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentLeft="true"
	                    android:text="收货仓库："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	
	                <TextView
	                    android:id="@+id/fh_ytms"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentRight="true"
	                    android:layout_alignParentTop="true"
	                    android:text="有条码数："
	                    android:textColor="#4169E1"
	                    android:textSize="@dimen/common_text_small" />
	
	            </RelativeLayout>
	
	            <RelativeLayout
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_marginBottom="@dimen/common_margin"
	                android:layout_marginLeft="@dimen/common_margin"
	                android:layout_marginRight="@dimen/common_margin" >
	
	                <TextView
	                    android:id="@+id/fh_djlx"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentLeft="true"
	                    android:text="单据类型："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	
	                <TextView
	                    android:id="@+id/fh_mtms"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentRight="true"
	                    android:layout_alignParentTop="true"
	                    android:text="没条码数："
	                    android:textColor="#DA70D6"
	                    android:textSize="@dimen/common_text_small" />
	
	            </RelativeLayout>
	
	            <RelativeLayout
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_marginBottom="@dimen/common_margin"
	                android:layout_marginLeft="@dimen/common_margin"
	                android:layout_marginRight="@dimen/common_margin" >
	
	                <TextView
	                    android:id="@+id/fh_zdr"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentLeft="true"
	                    android:text="制单人员："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	
	                <TextView
	                    android:id="@+id/fh_zdsj"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentRight="true"
	                    android:text="制单时间："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	
	            </RelativeLayout>
	        </LinearLayout>
	
	        <LinearLayout
	            android:id="@+id/sh_receipt_information"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:background="@drawable/sdl_background"
	            android:orientation="vertical" >
	
	            <RelativeLayout
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_margin="@dimen/common_margin" >
	
	                <TextView
	                    android:id="@+id/sh_shbh"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentLeft="true"
	                    android:text="收货编号："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	
	                <TextView
	                    android:id="@+id/sh_wlzs"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentRight="true"
	                    android:layout_alignParentTop="true"
	                    android:text="物料总数："
	                    android:textColor="#008B00"
	                    android:textSize="@dimen/common_text_small" />
	
	            </RelativeLayout>
	
	            <RelativeLayout
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_marginBottom="@dimen/common_margin"
	                android:layout_marginLeft="@dimen/common_margin"
	                android:layout_marginRight="@dimen/common_margin" >
	
	                <TextView
	                    android:id="@+id/sh_fhck"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentLeft="true"
	                    android:text="发货仓库："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	
	                <TextView
	                    android:id="@+id/sh_tmzs"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentRight="true"
	                    android:layout_alignParentTop="true"
	                    android:text="条码总数："
	                    android:textColor="#EE7600"
	                    android:textSize="@dimen/common_text_small" />
	
	            </RelativeLayout>
	
	            <RelativeLayout
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_marginBottom="@dimen/common_margin"
	                android:layout_marginLeft="@dimen/common_margin"
	                android:layout_marginRight="@dimen/common_margin" >
	
	                <TextView
	                    android:id="@+id/sh_jsck"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentLeft="true"
	                    android:text="接收仓库："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	
	                <TextView
	                    android:id="@+id/sh_wyztms"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentRight="true"
	                    android:layout_alignParentTop="true"
	                    android:text="未验证条码数："
	                    android:textColor="#FF0000"
	                    android:textSize="@dimen/common_text_small" />
	
	            </RelativeLayout>
	
	            <RelativeLayout
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_marginBottom="@dimen/common_margin"
	                android:layout_marginLeft="@dimen/common_margin"
	                android:layout_marginRight="@dimen/common_margin" >
	
	                <TextView
	                    android:id="@+id/sh_fhr"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentLeft="true"
	                    android:text="发货人员："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	
	                <TextView
	                    android:id="@+id/sh_fhsj"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentRight="true"
	                    android:text="发货时间："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	
	            </RelativeLayout>
	        </LinearLayout>
	
	        <LinearLayout
	            android:id="@+id/bx_guarantee_information"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:background="@drawable/sdl_background"
	            android:orientation="vertical" >
	
	            <RelativeLayout
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_margin="@dimen/common_margin" >
	
	                <TextView
	                    android:id="@+id/bx_bxdh"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentLeft="true"
	                    android:text="报修单号："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	            </RelativeLayout>
	
	            <RelativeLayout
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_marginBottom="@dimen/common_margin"
	                android:layout_marginLeft="@dimen/common_margin"
	                android:layout_marginRight="@dimen/common_margin" >
	
	                <TextView
	                    android:id="@+id/bx_bxcw"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentLeft="true"
	                    android:text="报修仓库："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	            </RelativeLayout>
	
	            <RelativeLayout
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_marginBottom="@dimen/common_margin"
	                android:layout_marginLeft="@dimen/common_margin"
	                android:layout_marginRight="@dimen/common_margin" >
	
	                <TextView
	                    android:id="@+id/bx_bxry"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentLeft="true"
	                    android:text="报修人员："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	
	                <TextView
	                    android:id="@+id/bx_bxsj"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_alignParentRight="true"
	                    android:text="保修时间："
	                    android:textColor="@color/text_666666"
	                    android:textSize="@dimen/common_text_small" />
	            </RelativeLayout>
	        </LinearLayout>
	
	        <RelativeLayout
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:background="@drawable/sdl_background"
	            android:paddingBottom="5dp"
	            android:paddingTop="5dp" >
	
	            <Button
	                android:id="@+id/ewm_opencame"
	                android:layout_width="match_parent"
	                android:layout_height="@dimen/common_btn_height"
	                android:layout_alignParentLeft="true"
	                android:layout_alignParentRight="true"
	                android:layout_centerHorizontal="true"
	                android:layout_centerVertical="true"
	                android:layout_weight="1"
	                android:background="@drawable/btn_red_bg"
	                android:minHeight="@null"
	                android:minWidth="@null"
	                android:text="开始扫描"
	                android:textColor="@color/white"
	                android:textSize="14sp" />
	
	        </RelativeLayout>
	    </LinearLayout>
	
	    <RelativeLayout
	        android:id="@+id/relativelayout01"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_alignParentBottom="true" >
	
	        <LinearLayout
	            android:id="@+id/ewm_btnlinearlayout"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:padding="@dimen/common_padding5" >
	
	            <Button
	                android:id="@+id/ewm_cancel"
	                android:layout_width="match_parent"
	                android:layout_height="@dimen/common_btn_height"
	                android:layout_marginRight="@dimen/common_margin"
	                android:layout_weight="1"
	                android:background="@drawable/btn_orange_bg"
	                android:minHeight="@null"
	                android:minWidth="@null"
	                android:text="关闭"
	                android:textColor="@color/white"
	                android:textSize="14sp" />
	
	            <Button
	                android:id="@+id/ewm_preservation"
	                android:layout_width="match_parent"
	                android:layout_height="@dimen/common_btn_height"
	                android:layout_marginLeft="@dimen/common_margin"
	                android:layout_weight="1"
	                android:background="@drawable/btn_green_bg"
	                android:minHeight="@null"
	                android:minWidth="@null"
	                android:text="保存"
	                android:textColor="@color/white"
	                android:textSize="14sp" />
	
	        </LinearLayout>
	    </RelativeLayout>
	
	    <RelativeLayout
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_above="@+id/relativelayout01"
	        android:layout_below="@+id/linearLayout1"
	        android:background="@drawable/sdl_background"
	        android:orientation="vertical" >
	
	        <ListView
	            android:id="@+id/qrcode_cargoscan_listview"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:cacheColorHint="@null"
	            android:divider="@null"
	            android:footerDividersEnabled="false" >
	
	        </ListView>
	
	        <include
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            layout="@layout/item_delivery_qrcode_smjl"
	            android:visibility="gone" />
	
	        <TextView
	            android:id="@+id/ewm_ts_text"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerHorizontal="true"
	            android:layout_centerInParent="true"
	            android:text="请扫描二维码"
	            android:textColor="@color/text_666666"
	            android:textSize="14sp"
	            android:visibility="gone" />
	
	    </RelativeLayout>
	
	</RelativeLayout>
	
</LinearLayout>